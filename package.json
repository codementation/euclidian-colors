{
  "name": "typescript-express",
  "version": "0.0.1",
  "description": "Spring Test",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cd build && node src",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "env-cmd": "^10.1.0",
    "fs": "0.0.1-security"
  }
}
